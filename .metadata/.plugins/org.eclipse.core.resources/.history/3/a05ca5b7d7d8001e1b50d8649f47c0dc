#ifndef CLOCK_CONFIG_HPP_
#define CLOCK_CONFIG_HPP_

#include "CMSIS/Device/ST/STM32F4xx/Include/system_stm32f4xx.h"

#include "CMSIS/Device/ST/STM32F4xx/Include/stm32f4xx.h"

#define PLL_N	180			//SYSTEM CLOCK SPEED (FCY (MHz))
#define HSI		16000000	//INTERAL OSC FREQUENCY

#define PLL_M	(HSI/2000000)		//Fcy = Fxtal x PLL_N/(PLL_P x PLL_M)
#define PLL_P	2
#define PLL_Q	7


void clock_config180(void);
void lcd_clock_config(void);




#endif /* CLOCK_CONFIG_HPP_ */
