#include "CMSIS/Device/ST/STM32F4xx/Include/stm32f4xx.h"
#include <stdio.h>
#include "UART.h"




int main(void)
{
	uart3_tx_init();

	setvbuf(stdin, NULL, _IONBF, 0);

	char str1[20],str2[20];

    /* Loop forever */
	while(1){
		printf("Enter name: ");
		scanf("%19[^\n]s", str1);

		printf("\n\rEnter your website name: ");
		scanf("%29[^\n]s", str2);

		printf("\n\rEntered Name: %s", str1);
		printf("\n\rEntered Website:%s\n\r", str2);
	}
}
